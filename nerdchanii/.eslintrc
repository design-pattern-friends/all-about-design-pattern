{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "naver",
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "prettier"
  ],
  "overrides": [],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "no-var": "error",
    "no-console": "off",
    "class-methods-use-this": "off"
  }
}