{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "strict": true,
    "target": "es6",
    "lib": [
      "ES5",
      "ES6",
      "ESNext",
      "dom"
    ],
    "skipLibCheck": true,
    "module": "commonjs",
    "isolatedModules": true,
    "forceConsistentCasingInFileNames": true,
  },
  "exclude": [],
}
