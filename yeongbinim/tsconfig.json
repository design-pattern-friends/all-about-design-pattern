{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "strict": true,
    "target": "es6",
    "lib": [
      "ES5",
      "ES6",
      "ESNext",
      "DOM"
    ],
    "skipLibCheck": true,
    "module": "CommonJS",
    "isolatedModules": false,
    "forceConsistentCasingInFileNames": true,
  },
  "exclude": [],
}