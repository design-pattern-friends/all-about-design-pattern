{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "strict": true,
    "target": "es6",
    "lib": [
      "ES5",
      "ES6",
      "ESNext"
    ],
    "skipLibCheck": true,
    "module": "ESNext",
    "isolatedModules": true,
    "forceConsistentCasingInFileNames": true,
  },
  "exclude": [
    "src/**/dist/**"
  ],
  "include": []
}